    "react-dom": "^19.2.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.9.6",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^4.4.5",
    "redux": "^5.0.1",
    "store2": "^2.14.4",
    "toastr": "^2.1.4"
